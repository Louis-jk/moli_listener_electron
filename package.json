{
  "name": "MOLI",
  "version": "0.1.0",
  "private": true,
  "author": "Joonho Kim",
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "agora-electron-sdk": "^3.7.0-build.519",
    "agora-rtc-react": "^1.1.1",
    "agora-rtc-sdk": "^3.6.10",
    "agora-rtc-sdk-ng": "^4.11.1",
    "axios": "^0.27.2",
    "electron-is-dev": "^2.0.0",
    "qs": "^6.10.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "start": "concurrently \"cross-env NODE_ENV=development BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "yarn react-build && electron-builder",
    "release": "yarn react-build && electron-builder --publish=always"
  },
  "agora_electron": {
    "electron_version": "10.2.0",
    "platform": "darwin",
    "prebuilt": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "agora-electron-sdk": "latest",
    "agora-rtc-sdk-ng": "latest",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "electron": "10.2.0",
    "electron-builder": "^23.0.3",
    "wait-on": "^6.0.1"
  },
  "build": {
    "productName": "모리(MOLI)",
    "appId": "com.moli",
    "files": [
      "build/**/*",
      "icon.png",
      "electron.js"
    ],
    "asar": true,
    "mac": {
      "target": [
        "default"
      ],
      "icon": "build/icons/mac/icon.icns"
    },
    "dmg": {
      "title": "모리(MOLI)",
      "icon": "build/icons/mac/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ],
      "icon": "build/icons/win/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "perMachine": true,
      "language": 1042,
      "installerIcon": "build/icons/win/icon.ico",
      "shortcutName": "모리(MOLI)",
      "artifactName": "모리(MOLI)-${version}.${ext}",
      "uninstallDisplayName": "모리(MOLI)"
    },
    "directories": {
      "buildResources": "./build/",
      "output": "./dist/"
    }
  }
}
